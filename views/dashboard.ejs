<html>
    <h1>Dashboard</h1>
    <p>Welcome to your dashboard! <%= title %></p>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="/styles.css">
    <table class="table table-striped table-bordered table-dark">
        <thead>
            <tr>
                <th>Name</th>
                <th>Email</th>
                <th>Password</th>
                <th>Role</th>
            </tr>
        </thead>
        <tbody>
            <% users.forEach(user => { %>
                <tr>
                    <td><%= user.name %></td>
                    <td><%= user.email %></td>
                    <td><%= user.password %></td>
                    <td><%= user.role %></td>
                </tr>
            <% }); %>
        </tbody>
    </table>
</html>